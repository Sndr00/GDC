#include <stdio.h>
#include <stdlib.h>

int main (){
int a [5][5]={{0,0,7,1,0},{1,2,4,5,1},{2,4,2,3,2},{3,6,0,4,3},{4,8,2,5,4}};
int i, suma=0;

for (i=0;i<5;i++){
    printf("   %d",a[i][0]);
    printf("   %d",a[i][1]);
    printf("   %d",a[i][2]);
    printf("   %d",a[i][3]);
    printf("   %d\n",a[i][4]);
    }   

suma=a[0][0]+a[1][1]+a[2][2]+a[3][3]+a[4][4];
printf("\nLa suma de la diagonal principal es: %d",suma);

return 0;
}
